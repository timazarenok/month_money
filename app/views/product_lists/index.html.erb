<p id="notice"><%= notice %></p>

<h1>Product Lists</h1>

<table>
  <thead>
    <tr>
      <th>Month</th>
      <th>Year</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_lists.each do |product_list| %>
      <tr>
        <td><%= product_list.month %></td>
        <td><%= product_list.year %></td>
        <td><%= product_list.value %></td>
        <td><%= link_to 'Show', product_list %></td>
        <td><%= link_to 'Edit', edit_product_list_path(product_list) %></td>
        <td><%= link_to 'Destroy', product_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product List', new_product_list_path %>
